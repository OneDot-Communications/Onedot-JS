{
  "name": "@onedot/mobile",
  "version": "1.0.12",
  "description": "Mobile application support for ONEDOT-JS framework",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "build:android": "cd android && ./gradlew assembleDebug",
    "build:ios": "cd ios && xcodebuild -workspace ONEDOT.xcworkspace -scheme ONEDOT -configuration Debug",
    "run:android": "cd android && ./gradlew installDebug",
    "run:ios": "cd ios && xcodebuild -workspace ONEDOT.xcworkspace -scheme ONEDOT -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 14,OS=16.0' install",
    "deploy:android": "cd android && ./gradlew assembleRelease",
    "deploy:ios": "cd ios && xcodebuild -workspace ONEDOT.xcworkspace -scheme ONEDOT -configuration Release archive"
  },
  "dependencies": {
    "@onedot/core": "^1.0.12",
    "react-native": "^0.71.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.71.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "pod-install": "^0.1.38",
    "rimraf": "^4.4.0",
    "typescript": "^5.0.0"
  },
  "author": "ONEDOT-JS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/onedot-js/onedot-js.git",
    "directory": "packages/mobile"
  },
  "keywords": [
    "onedot-js",
    "mobile",
    "react-native",
    "cross-platform",
    "ios",
    "android"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-native": "^0.71.0"
  }
}
