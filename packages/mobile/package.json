{
  "name": "@onedot/mobile",
  "version": "1.0.0",
  "description": "Mobile application support for ONEDOT-JS framework",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "build:android": "cd android && ./gradlew assembleDebug",
    "build:ios": "cd ios && xcodebuild -workspace ONEDOT.xcworkspace -scheme ONEDOT -configuration Debug",
    "run:android": "cd android && ./gradlew installDebug",
    "run:ios": "cd ios && xcodebuild -workspace ONEDOT.xcworkspace -scheme ONEDOT -configuration Debug -destination 'platform=iOS Simulator,name=iPhone 14,OS=16.0' install",
    "deploy:android": "cd android && ./gradlew assembleRelease",
    "deploy:ios": "cd ios && xcodebuild -workspace ONEDOT.xcworkspace -scheme ONEDOT -configuration Release archive"
  },
  "dependencies": {
    "@onedot/core": "^1.0.0",
    "react-native": "^0.71.0",
    "react-native-webview": "^11.26.0",
    "react-native-screens": "^3.20.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-reanimated": "^2.17.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-device-info": "^10.3.0",
    "react-native-async-storage": "^1.17.11",
    "react-native-netinfo": "^9.3.7",
    "react-native-permissions": "^3.6.1",
    "react-native-camera": "^4.2.1",
    "react-native-geolocation": "^2.1.0",
    "react-native-push-notification": "^8.1.1",
    "react-native-firebase": "^17.4.2",
    "react-native-code-push": "^7.0.4",
    "react-native-splash-screen": "^3.3.0",
    "react-native-bootsplash": "^4.3.2",
    "react-native-localize": "^2.2.4",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-svg": "^13.9.0",
    "react-native-video": "^5.2.1",
    "react-native-image-picker": "^4.10.0",
    "react-native-share": "^8.2.2",
    "react-native-in-app-browser": "^3.0.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-actions-sheet": "^2.4.2",
    "react-native-slider": "^4.4.2",
    "react-native-segmented-control": "^2.4.0",
    "react-native-tab-view": "^3.3.0",
    "react-native-pager-view": "^6.1.2",
    "react-native-drawer-layout": "^3.2.1",
    "react-native-bottom-tabs": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.0",
    "@types/react-native": "^0.71.0",
    "typescript": "^5.0.0",
    "jest": "^29.5.0",
    "@types/jest": "^29.5.0",
    "rimraf": "^4.4.0",
    "eslint": "^8.39.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "metro-react-native-babel-preset": "^0.76.0",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@babel/runtime": "^7.21.5",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "jetifier": "^2.0.0",
    "pod-install": "^0.1.38"
  },
  "author": "ONEDOT-JS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/onedot-js/onedot-js.git",
    "directory": "packages/mobile"
  },
  "keywords": [
    "onedot-js",
    "mobile",
    "react-native",
    "cross-platform",
    "ios",
    "android"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-native": "^0.71.0"
  }
}
