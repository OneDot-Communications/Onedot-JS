{
  "name": "@onedot/docs",
  "version": "1.0.0",
  "description": "Documentation generator and tools for ONEDOT-JS framework",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "clean": "rimraf dist",
    "generate": "node dist/generator/index.js",
    "generate-api": "node dist/generator/index.js --api",
    "generate-guides": "node dist/generator/index.js --guides",
    "generate-tutorials": "node dist/generator/index.js --tutorials",
    "serve": "node dist/server.js",
    "preview": "npm run build && npm run serve"
  },
  "dependencies": {
    "@onedot/core": "^1.0.0",
    "marked": "^5.0.0",
    "highlight.js": "^11.8.0",
    "fs-extra": "^11.1.1",
    "glob": "^10.2.2",
    "yaml": "^2.3.1",
    "front-matter": "^4.0.2",
    "lodash": "^4.17.21",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "open": "^8.4.2",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.194",
    "@types/marked": "^5.0.0",
    "@types/ws": "^8.5.5",
    "typescript": "^5.0.0",
    "jest": "^29.5.0",
    "@types/jest": "^29.5.0",
    "rimraf": "^4.4.0",
    "eslint": "^8.39.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0"
  },
  "author": "ONEDOT-JS Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/onedot-js/onedot-js.git",
    "directory": "packages/docs"
  },
  "keywords": [
    "onedot-js",
    "documentation",
    "docs",
    "generator",
    "api",
    "guides",
    "tutorials"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
